{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "foreground": "#ffffff",
          "background": "#cc3802",
          "template": "{{ if .Env.PNPPSHOST }} \uf8c5 {{ .Env.PNPPSHOST }} {{ end }}"
        },
        {
          "type": "text",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#047e84",
          "powerline_symbol": "\ue0b0",
          "template": "{{ if .Env.PNPPSSITE }} \uf672 {{ .Env.PNPPSSITE }}{{ end }}"
        },
        {
          "type": "text",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#047e84",
          "template": "{{ if .Env.PNPPSSITE }}\u00A0{{ end }}"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#ef5350",
          "foreground": "#ffffff",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}<#262626> </>",
          "type": "os"
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#000000",
          "background": "#0B59E7",
          "template": "  {{ .Context }} "
        },
        {
          "background": "#FF9248",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "short"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#33658A",
          "template": " <#fefefe>ﮫ</> {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin"
          }
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "style": "diamond",
          "trailing_diamond": "\ue0b4 ",
          "template": " {{ .UserName }} \u2665",
          "type": "session"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#F1502F",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": false,
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "fetch_worktree_count": false
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "python"
        },
        {
          "background": "#512bd4",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "style": "diamond",
          "template": "\ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}",
          "type": "dotnet"
        },
        {
          "background": "#565656",
          "foreground": "#faa029",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "trailing_diamond": "\ue0b4 ",
          "type": "aws"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#9A348E",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#e0f8ff",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ufb00 ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}